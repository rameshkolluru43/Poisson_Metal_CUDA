#!/bin/zsh
# Clean all solution files, plots, and executables in the workspace

echo "Cleaning solution files..."
rm -f solution*.csv solution*.dat

echo "Cleaning intermediate files..."
rm -f *.o *.dSYM

echo "Cleaning plot scripts and images..."
rm -f *.png

echo "Cleaning executables..."
rm -f a.out *.out *.exe

echo "Cleaning plot scripts..."
rm -f *.gp

echo "Cleaning csv files ..."
rm -f *.csv
echo "Cleaning executables..."
rm -f a.out *.out *.exe

echo "Clean complete."
